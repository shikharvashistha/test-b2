using asciidoctor ;

html ublas.html : ublas.adoc ;

install html_ : ublas.html : <location>../ ;

pdf ublas.pdf : ublas.adoc ;

explicit ublas.pdf ;

install pdf_ : ublas.pdf : <location>pdf ;

explicit pdf_ ; 

alias boostdoc ;

explicit boostdoc ;

alias boostrelease : html_ ;

explicit boostrelease ;