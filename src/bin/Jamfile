using asciidoctor ;

html index.html : index.adoc ;

install html_ : index.html : <location>html ;

pdf index.pdf : index.adoc ;

explicit index.pdf ;

install pdf_ : index.pdf : <location>pdf ;

explicit pdf_ ;

alias boostdoc ;

explicit boostdoc ;

alias boostrelease : html_ ;

explicit boostrelease ;